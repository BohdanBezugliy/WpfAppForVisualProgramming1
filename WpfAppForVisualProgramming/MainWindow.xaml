<Window x:Class="WpfAppForVisualProgramming.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppForVisualProgramming"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Ellipse Fill="Red" Width="200" Height="200"></Ellipse>
        </StackPanel>
        <Rectangle Fill="Aqua" Width="200" Height="80" RadiusX="20" RadiusY="20"></Rectangle>
    </Grid>
</Window>
