<Window x:Class="WpfAppForVisualProgramming.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppForVisualProgramming"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Name="grid1">
        <Path Stroke="Black" Fill="Aqua">
            <Path.Data>
                <GeometryGroup FillRule="Nonzero">
                    <LineGeometry StartPoint="10,20" EndPoint="700,20"></LineGeometry>
                    <EllipseGeometry Center="150,200" RadiusX="100" RadiusY="40"></EllipseGeometry>
                    <RectangleGeometry Rect="190,200 80,20" RadiusX="5" RadiusY="5"></RectangleGeometry>
                </GeometryGroup>
            </Path.Data>
        </Path>
    </Grid>
</Window>
